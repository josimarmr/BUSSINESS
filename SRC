  <div class="reveal reveal-delay-2" style="position: relative;">
    <p style="color: var(--text2); font-size: 0.9rem;">
      Apresentado por <strong style="color: var(--text);">Josimar Silva</strong> &
      <strong style="color: var(--text);">Mario Michalski</strong>
    </p>
  </div>
</section>

<footer>
  <p>CLIENTÃO v2.0 — Business Plan Confidencial — Fevereiro 2026</p>
</footer>

<script>
// SCROLL REVEAL
const observer = new IntersectionObserver((entries) => {
  entries.forEach(entry => { if (entry.isIntersecting) entry.target.classList.add('visible'); });
}, { threshold: 0.1, rootMargin: '0px 0px -50px 0px' });
document.querySelectorAll('.reveal').forEach(el => observer.observe(el));

// ACTIVE NAV
const sections = document.querySelectorAll('section[id]');
window.addEventListener('scroll', () => {
  let current = '';
  sections.forEach(s => { if (window.scrollY >= s.offsetTop - 200) current = s.id; });
  document.querySelectorAll('.nav-links a').forEach(a => a.classList.toggle('active', a.getAttribute('href') === '#' + current));
});

// CONFIDENTIAL TOGGLE
let confidentialOpen = false;
function openConfidential() {
  document.getElementById('confidentialContent').classList.add('visible');
  document.getElementById('confidentialGate').style.display = 'none';
  confidentialOpen = true;
  // Build ROI chart
  setTimeout(buildROIChart, 100);
}
function closeConfidential() {
  document.getElementById('confidentialContent').classList.remove('visible');
  document.getElementById('confidentialGate').style.display = 'block';
  confidentialOpen = false;
}

// ROI CHART
function buildROIChart() {
  const chart = document.getElementById('roiChart');
  if (chart.children.length > 0) return; // already built

  const data = [
    { m: 'M1', c: 38500, r: 3500 }, { m: 'M2', c: 38500, r: 7000 },
    { m: 'M3', c: 38500, r: 12250 }, { m: 'M4', c: 38500, r: 19250 },
    { m: 'M5', c: 38500, r: 28000 }, { m: 'M6', c: 38500, r: 38500 },
    { m: 'M7', c: 35000, r: 45500 }, { m: 'M8', c: 35000, r: 52500 },
    { m: 'M9', c: 35000, r: 59500 }, { m: 'M10', c: 35000, r: 63000 },
    { m: 'M11', c: 35000, r: 66500 }, { m: 'M12', c: 35000, r: 70000 }
  ];
  const max = Math.max(...data.map(d => Math.max(d.c, d.r)));

  data.forEach((d, i) => {
    const g = document.createElement('div'); g.className = 'roi-bar-group';
    const cont = document.createElement('div'); cont.className = 'roi-bar-container';

    const cb = document.createElement('div'); cb.className = 'roi-bar cost'; cb.style.height = '0px';
    setTimeout(() => { cb.style.height = (d.c / max * 180) + 'px'; }, 200 + i * 80);
    const cv = document.createElement('div'); cv.className = 'roi-bar-value'; cv.textContent = (d.c/1000).toFixed(0)+'K';
    cb.appendChild(cv);

    const rb = document.createElement('div'); rb.className = 'roi-bar revenue'; rb.style.height = '0px';
    setTimeout(() => { rb.style.height = (d.r / max * 180) + 'px'; }, 300 + i * 80);
    const rv = document.createElement('div'); rv.className = 'roi-bar-value'; rv.textContent = (d.r/1000).toFixed(1)+'K';
    rb.appendChild(rv);

    cont.appendChild(cb); cont.appendChild(rb);
    const lb = document.createElement('div'); lb.className = 'roi-month'; lb.textContent = d.m;
    g.appendChild(cont); g.appendChild(lb); chart.appendChild(g);
  });
}
</script>
</body>
</html>
